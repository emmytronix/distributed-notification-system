NODE_ENV=development
PORT=3003

# Message Queue
RABBITMQ_URL=amqp://rabbit_user:rabbit_password@localhost:5672

# Cache (Primary storage for this service)
REDIS_URL=redis://localhost:6379

# Service URLs
TEMPLATE_SERVICE_URL=http://localhost:3004

# Audit Trail Database (shared - write only)
NOTIFICATION_LOGS_DB_URL=postgresql://notif_admin:notif_admin_password@localhost:5432/notification_logs_db

# Firebase Cloud Messaging
FCM_SERVER_KEY=your-fcm-server-key

# Retry Configuration
MAX_RETRIES=3
RETRY_BASE_DELAY=1000

# Logging
LOG_LEVEL=info
